	<parameter name="KODE_THN_AJARAN" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select a.nm_jenis_ketenagaan,
       (
        select count(*)
        from t_pegawai_aktif as b
        where (b.kd_jenis_ketenagaan = a.kd_jenis_ketenagaan) and
              (b.kd_tahun_ajaran = $P{KODE_THN_AJARAN} )
       ) as jumlah
from r_jenis_ketenagaan as a
where a.kd_jenis_ketenagaan in ('1','2','6')
group by a.nm_jenis_ketenagaan]]>
	</queryString>
	<field name="nm_jenis_ketenagaan" class="java.lang.String"/>
	<field name="jumlah" class="java.lang.Long"/>
	<variable name="NO" class="java.lang.Integer" calculation="Count">
		<variableExpression><![CDATA[$V{NO}+1]]></variableExpression>
		<initialValueExpression><![CDATA[1]]></initialValueExpression>
	</variable>
	<variable name="TOTAL" class="java.lang.Long" calculation="Sum">
		<variableExpression><![CDATA[$F{jumlah}]]></variableExpression>
	</variable>
