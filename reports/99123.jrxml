	<parameter name="KODE_THN_AJARAN" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT	CONCAT('TAHUN PELAJARAN : ', b.nm_tahun_ajaran) AS nm_tahun_ajaran
,	c.nm_tingkat_kelas		AS nm_tingkat_kelas
,	a.kd_rombel		AS kd_rombel
,	CONCAT(' ', d.nm_pegawai)	AS wali_kelas
,	CONCAT(' ', e.nm_pegawai)	AS guru_bk
,	CONCAT(' ', a.keterangan)	AS keterangan
FROM	t_pegawai_rombel	AS a
,	r_tahun_ajaran	AS b
,	r_tingkat_kelas	AS c
,	t_pegawai		AS d
,	t_pegawai		AS e
WHERE	a.kd_tahun_ajaran	= b.kd_tahun_ajaran
AND	a.kd_tingkat_kelas	= c.kd_tingkat_kelas
AND	a.id_pegawai	= d.id_pegawai
AND	a.id_pegawai_bk	= e.id_pegawai
AND	a.kd_tahun_ajaran	= $P{KODE_THN_AJARAN}
ORDER BY	a.kd_tingkat_kelas
,	a.kd_rombel]]>
