<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="5335_sr4" pageWidth="555" pageHeight="802" columnWidth="555" leftMargin="0" rightMargin="0" topMargin="0" bottomMargin="0">
	<property name="ireport.zoom" value="1.0"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="KODE_THN_AJARAN" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT	kd_tahun_ajaran,
	v_siswa_putus_sekolah.kd_tingkat_kelas,
	r_tingkat_kelas.nm_tingkat_kelas,
	count(1) as jml
FROM	v_siswa_putus_sekolah, r_tingkat_kelas
WHERE	kd_tahun_ajaran = $P{KODE_THN_AJARAN}
and 	v_siswa_putus_sekolah.kd_tingkat_kelas = r_tingkat_kelas.kd_tingkat_kelas
group by	v_siswa_putus_sekolah.kd_tingkat_kelas, r_tingkat_kelas.nm_tingkat_kelas]]>
	</queryString>
	<field name="kd_tahun_ajaran" class="java.lang.String"/>
	<field name="kd_tingkat_kelas" class="java.lang.String"/>
	<field name="nm_tingkat_kelas" class="java.lang.String"/>
	<field name="jml" class="java.lang.Long"/>
	<background>
		<band splitType="Stretch"/>
	</background>
	<summary>
