	<queryString>
		<![CDATA[select  	id_pegawai as id_pegawai,
	    	nm_pegawai as nama,
			kd_jns_kelamin,
			v_pegawai_detil.kd_jenis_pegawai,
			v_pegawai_detil.kd_pangkat,
			v_pegawai_detil.kd_tenaga_administrasi,
			no_sk,
			tgl_sk,
			tmt_sk,
			v_pegawai_detil.kd_status_pegawai,
			ifnull(umur,0) umur,
    		ifnull(ms_krj_awal,0) ms_krj_awal,
             nm_tenaga_administrasi,
             nm_jenis_pegawai,
             nm_pangkat,
             nm_status_pegawai
from 	v_pegawai_detil, r_tenaga_administrasi, r_jenis_pegawai, r_pangkat, r_status_pegawai
where (kd_jenis_ketenagaan in ('2')) and
		(sts_aktif = '1') and
		kd_tahun_ajaran = $P{KODE_THN_AJARAN} and
     v_pegawai_detil.kd_tenaga_administrasi = r_tenaga_administrasi.kd_tenaga_administrasi and
     v_pegawai_detil.kd_jenis_pegawai = r_jenis_pegawai.kd_jenis_pegawai and
     v_pegawai_detil.kd_pangkat = r_pangkat.kd_pangkat and
     v_pegawai_detil.kd_status_pegawai = r_status_pegawai.kd_status_pegawai
order by kd_pangkat desc, ms_krj_sk desc, ms_krj_awal desc]]>
