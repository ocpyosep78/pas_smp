	<parameter name="KODE_THN_AJARAN" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT	CONCAT('TAHUN PELAJARAN : ', c.nm_tahun_ajaran)	AS nm_tahun_ajaran
,	b.nis			AS nis
,	CONCAT(' ', b.nm_siswa) 	AS nm_siswa
,	b.kd_jenis_kelamin		AS kd_jenis_kelamin
,	CONCAT(' ', b.alamat)	AS alamat
FROM	t_siswa_tingkat	AS a
,	t_siswa		AS b
,	r_tahun_ajaran	AS c
WHERE	a.id_siswa		= b.id_siswa
AND	a.kd_tahun_ajaran	= c.kd_tahun_ajaran
AND	a.kd_lulus		= '1'
AND	a.kd_tingkat_kelas	= '03'
AND	b.status_siswa	in ('0','2','3','6')
AND	a.kd_tahun_ajaran	= $P{KODE_THN_AJARAN}]]>
	</queryString>
